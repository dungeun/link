    "@modules/types": "workspace:*",
    "@modules/utils": "workspace:*",
    "@modules/storage": "workspace:*",
    "@modules/notification": "workspace:*",
    "crypto": "^1.0.1",
    "date-fns": "^2.30.0",
    "handlebars": "^4.7.8",
    "pdfkit": "^0.14.0",
    "uuid": "^9.0.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/pdfkit": "^0.13.0",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitest/ui": "^1.0.0",
    "eslint": "^8.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
