    "lint": "eslint src --ext .ts,.tsx",
    "format": "prettier --write \"src/**/*.{ts,tsx}\""
  },
  "dependencies": {
    "@repo/api-client": "workspace:*",
    "@repo/auth-core": "workspace:*",
    "@repo/cache": "workspace:*",
    "@repo/core": "workspace:*",
    "@repo/storage": "workspace:*",
    "@repo/types": "workspace:*",
    "@repo/utils": "workspace:*",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zod": "^3.22.4"
