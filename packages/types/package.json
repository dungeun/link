{
  "name": "@repo/types",
  "version": "1.0.0",
  "description": "Enterprise AI Module System - Common Types",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./common": {
      "types": "./dist/common.d.ts",
      "import": "./dist/common.js",
      "require": "./dist/common.js"
    },
    "./auth": {
      "types": "./dist/auth.d.ts",
      "import": "./dist/auth.js",
      "require": "./dist/auth.js"
    },
    "./business": {
      "types": "./dist/business.d.ts",
      "import": "./dist/business.js",
      "require": "./dist/business.js"
    },
    "./ui": {
      "types": "./dist/ui.d.ts",
      "import": "./dist/ui.js",
      "require": "./dist/ui.js"
    },
    "./schemas": {
      "types": "./dist/schemas.d.ts",
      "import": "./dist/schemas.js",
      "require": "./dist/schemas.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "types",
    "typescript",
    "enterprise",
    "ai-development"
  ],
  "author": "Enterprise AI Team",
  "license": "MIT",
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0",
    "eslint": "^8.55.0",
    "@types/node": "^20.10.0"
  },
  "dependencies": {
    "zod": "^3.22.0"
  }
}