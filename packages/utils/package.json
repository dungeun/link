{
  "name": "@repo/utils",
  "version": "1.0.0",
  "description": "Enterprise AI Module System - Utility Functions",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./crypto": {
      "types": "./dist/crypto.d.ts",
      "import": "./dist/crypto.js",
      "require": "./dist/crypto.js"
    },
    "./date": {
      "types": "./dist/date.d.ts",
      "import": "./dist/date.js",
      "require": "./dist/date.js"
    },
    "./file": {
      "types": "./dist/file.d.ts",
      "import": "./dist/file.js",
      "require": "./dist/file.js"
    },
    "./format": {
      "types": "./dist/format.d.ts",
      "import": "./dist/format.js",
      "require": "./dist/format.js"
    },
    "./http": {
      "types": "./dist/http.d.ts",
      "import": "./dist/http.js",
      "require": "./dist/http.js"
    },
    "./string": {
      "types": "./dist/string.d.ts",
      "import": "./dist/string.js",
      "require": "./dist/string.js"
    },
    "./validation": {
      "types": "./dist/validation.d.ts",
      "import": "./dist/validation.js",
      "require": "./dist/validation.js"
    },
    "./async": {
      "types": "./dist/async.d.ts",
      "import": "./dist/async.js",
      "require": "./dist/async.js"
    },
    "./array": {
      "types": "./dist/array.d.ts",
      "import": "./dist/array.js",
      "require": "./dist/array.js"
    },
    "./object": {
      "types": "./dist/object.d.ts",
      "import": "./dist/object.js",
      "require": "./dist/object.js"
    }
  },
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "utils",
    "utilities",
    "typescript",
    "enterprise",
    "ai-development"
  ],
  "author": "Enterprise AI Team",
  "license": "MIT",
  "dependencies": {
    "@repo/core": "workspace:*",
    "@repo/types": "workspace:*",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0",
    "eslint": "^8.55.0",
    "@types/node": "^20.10.0",
    "@types/crypto-js": "^4.2.0",
    "crypto-js": "^4.2.0"
  }
}