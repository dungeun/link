#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/admin/new_project/revu-platform/node_modules/.pnpm/knex@3.1.0_mysql2@3.14.2_pg@8.16.3_sqlite3@5.1.7/node_modules/knex/bin/node_modules:/Users/admin/new_project/revu-platform/node_modules/.pnpm/knex@3.1.0_mysql2@3.14.2_pg@8.16.3_sqlite3@5.1.7/node_modules/knex/node_modules:/Users/admin/new_project/revu-platform/node_modules/.pnpm/knex@3.1.0_mysql2@3.14.2_pg@8.16.3_sqlite3@5.1.7/node_modules:/Users/admin/new_project/revu-platform/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/admin/new_project/revu-platform/node_modules/.pnpm/knex@3.1.0_mysql2@3.14.2_pg@8.16.3_sqlite3@5.1.7/node_modules/knex/bin/node_modules:/Users/admin/new_project/revu-platform/node_modules/.pnpm/knex@3.1.0_mysql2@3.14.2_pg@8.16.3_sqlite3@5.1.7/node_modules/knex/node_modules:/Users/admin/new_project/revu-platform/node_modules/.pnpm/knex@3.1.0_mysql2@3.14.2_pg@8.16.3_sqlite3@5.1.7/node_modules:/Users/admin/new_project/revu-platform/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../knex/bin/cli.js" "$@"
else
  exec node  "$basedir/../knex/bin/cli.js" "$@"
fi
