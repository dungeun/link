{
  "name": "linkpick-web",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "NODE_OPTIONS='--max-old-space-size=4096' PORT=$(node scripts/find-free-port.js) && echo \"ðŸš€ Starting REVU Platform on port $PORT\" && next dev -p $PORT",
    "dev:clean": "rm -rf .next && npm run dev",
    "build": "prisma generate && npm run cache:generate && next build",
    "build:analyze": "prisma generate && ANALYZE=true next build",
    "cache:generate": "node scripts/generate-cache.js",
    "cache:refresh": "node scripts/generate-cache.js",
    "start": "next start",
    "lint": "next lint",
    "lint:security": "eslint . --config .eslintrc.security.js --ext .ts,.tsx",
    "type-check": "tsc --noEmit",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,md}\"",
    "clean": "rm -rf .next",
    "stop": "node scripts/stop-all-ports.js",
    "restart": "npm run stop && npm run dev",
    "reset-rate-limit": "node scripts/reset-rate-limit.js",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:seed": "ts-node prisma/seed-sample-data.ts",
    "db:seed-korean": "ts-node prisma/clear-and-seed-korean.ts",
    "db:seed-pricing": "ts-node prisma/seed-pricing.ts",
    "db:seed-templates": "ts-node prisma/seed-templates.ts",
    "db:seed-real": "ts-node prisma/seed-real-accounts.ts",
    "db:remove-placeholders": "ts-node prisma/remove-placeholder-images.ts",
    "db:verify": "ts-node prisma/verify-data.ts",
    "memory": "node scripts/optimize-memory.js",
    "memory:watch": "node scripts/optimize-memory.js --watch",
    "memory:gc": "node --expose-gc scripts/optimize-memory.js --gc",
    "dev:optimized": "NODE_OPTIONS='--max-old-space-size=4096 --expose-gc' npm run dev",
    "db:update-images": "ts-node prisma/update-campaign-images.ts",
    "db:migrate-images": "node scripts/migrate-images-to-local.js",
    "studio": "STUDIO_PORT=$(node scripts/find-free-port.js 5555) && echo \"ðŸ“Š Starting Prisma Studio on port $STUDIO_PORT\" && prisma studio --port $STUDIO_PORT",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:qa": "TEST_BUSINESS_EMAIL=business@demo.com TEST_BUSINESS_PASSWORD=password123 TEST_INFLUENCER_EMAIL=influencer@demo.com TEST_INFLUENCER_PASSWORD=password123 TEST_ADMIN_EMAIL=admin@demo.com TEST_ADMIN_PASSWORD=admin123! npx playwright test",
    "test:qa:headed": "npm run test:qa -- --headed",
    "test:qa:debug": "npm run test:qa -- --debug",
    "test:api": "npm run test:qa -- --project=api-tests",
    "test:ui": "npm run test:qa -- --project=ui-tests",
    "test:admin": "npm run test:qa -- --project=admin-tests",
    "test:auth": "npm run test:qa -- --project=auth-setup",
    "test:performance": "node scripts/performance-test.js",
    "test:performance:dev": "npm run dev & sleep 10 && npm run test:performance; pkill -f 'next dev'",
    "test:integration": "npm run test:qa -- --project=integration-tests",
    "test:e2e:cetimil": "npm run test:qa -- tests/e2e/cetimil-campaign-flow.spec.ts"
  },
